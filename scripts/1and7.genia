fn body
    () when NR == 1             -> NR 
  | () when NR == 19            -> NR 
  | () when $7 ~ r"^[0-9]+$"    -> NR
  | () when $7 ~ r"^[A-Z]"      -> 
        (
            open = "(";
            close = ")";
            comma = ",";
            FS = "";
            print( open, $1,comma, "'", $7 , "'", close) 
        )
  | ()                          -> NR
